<%= partial 'partials/info_navbar' %>

<section class="skills">
  <div class="container">
    <div class="row align-items-center">
        <div class="skills-content">
            <div class="row">
                <div class="section-title">
                    <h1>Friends 4 Benefits</h1>
                    <p>
                      A social app which helps people find new friends through mutual hobbies. People can look for new friends or offer an activity to invite others to join them. Features include calendar booking, image uploading, and private chat.
                    </p>
                    <div class="tech-stack">
                      <% if data.projects[5].rails? %>
                        <i class="devicon-rails-plain-wordmark tech"></i>
                      <% end %>
                      <% if data.projects[5].html? %>
                        <i class="devicon-html5-plain-wordmark tech"></i>
                      <% end %>
                      <% if data.projects[5].css? %>
                        <i class="devicon-css3-plain-wordmark tech"></i>
                      <% end %>
                      <% if data.projects[5].javascript? %>
                        <i class="devicon-javascript-plain tech"></i>
                      <% end %>
                      <% if data.projects[5].sql? %>
                        <i class="devicon-azuresqldatabase-plain tech"></i>
                      <% end %>
                      <% if data.projects[5].figma? %>
                        <i class="devicon-figma-plain tech"></i>
                      <% end %>
                    </div>
                    <div class="project-links">
                      <% if data.projects[5].code? %>
                        <% link_to data.projects[5].github, target: '_blank' do %>
                          <i class="fa-brands fa-github project-link"></i>
                        <% end %>
                      <% end %>
                      <% if data.projects[5].online? %>
                        <% link_to data.projects[5].url, class: 'globe', target: '_blank' do %>
                          <i class="fa-solid fa-globe project-link"></i>
                        <% end %>
                      <% end %>
                    </div>
                </div>
            </div>
        </div>
        <div class="img">
          <%= image_tag 'projects/friends4benefits/f4b_homepage.png' %>
          <!--<img src="images/skill.jpg" alt="skill">-->
        </div>
    </div>
  </div>
</section>

<div class="gallery-wrap">
  <i class="fa-solid fa-circle-chevron-left info-arrow" id="backBtn"></i>
    <div class="gallery">
      <div>
        <span><%= image_tag 'projects/friends4benefits/signup.png' %></span>
        <span><%= image_tag 'projects/friends4benefits/some_services.png' %></span>
        <span><%= image_tag 'projects/friends4benefits/emo_buddies.png' %></span>
      </div>
      <div>
        <span><%= image_tag 'projects/friends4benefits/all_buddies.png' %></span>
        <span><%= image_tag 'projects/friends4benefits/friend_bookings.png' %></span>
        <span><%= image_tag 'projects/friends4benefits/drink_buddy.png' %></span>
      </div>
    </div>
  <i class="fa-solid fa-circle-chevron-right info-arrow" id="nextBtn"></i>
</div>

